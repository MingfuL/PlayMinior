<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="TwitterStyleLike">
        <attr name="circle_start_color" format="color|reference" />
        <attr name="circle_end_color" format="color|reference" />
        <attr name="liked" format="boolean"/>
        <attr name="icon_size" format="dimension|reference"/>
    </declare-styleable>

    <declare-styleable name="CoolFloatingButton">
        <attr name="expand_orientation" format="enum">
            <enum name="top" value="0"/>
            <enum name="bottom" value="1"/>
            <enum name="left" value="2"/>
            <enum name="right" value="03"/>
        </attr>
    </declare-styleable>

</resources>